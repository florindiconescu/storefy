<% @bookings.each do |booking| %>
<p>User: <%= booking.user.first_name %></p>
<p>Status: <%= booking.status %></p>
<p>Title: <%= booking.storage.title %></p>
<p>Price: <%= booking.total_price %></p>
<p>Check-in: <%= booking.start_date %> </p>
<p>Check-out: <%= booking.end_date %> </p>
<p>Duration: <%= booking.end_date - booking.start_date %></p>
<p>Belongs to: <%= booking.storage.user.first_name %> </p>
<br>
<% end %>

