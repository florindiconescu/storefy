<h1>Edit <%= @storage.title %>?</h1>
<%= simple_form_for @storage do |f| %>
  <p><%= f.input :title %></p>
  <p><%= f.input :address %></p>
  <p><%= f.input :sqm %></p>
  <p><%= f.input :description %></p>
  <p><%= f.input :price %></p>
  <%= f.button :submit, class: "btn-outline-success" %>
  <%= link_to 'Add some pictures', storage_photos_path(@storage), class: "btn btn-outline-info" %>
  <%= link_to 'Back to storages', my_storages_path, class: "btn btn-outline-info" %>
<% end %>
